<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    package="com.example.stephen.meisr_mockup">

    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/meisr_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/meisr_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Mockup1"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Explaination"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".ModuleSelection"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".NewSurvey"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".CreateAccount"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".DisplayGraphs"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".DisplayModule"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".CheckSubmit"
            android:screenOrientation="portrait">
        </activity>

    </application>

</manifest>