<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RadioGroup
        android:id="@+id/radioGroup4"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/contbut"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup3"
        tools:orientation="horizontal">

        <RadioButton
            android:id="@+id/radiobutton13"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="217dp"
            tools:layout_editor_absoluteY="70dp" />

        <RadioButton
            android:id="@+id/radiobutton14"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1" />

        <RadioButton
            android:id="@+id/radiobutton15"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1" />

    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup3"
        android:layout_width="120dp"
        android:layout_height="51dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/radioGroup4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup2"
        tools:orientation="horizontal">

        <RadioButton
            android:id="@+id/radiobutton9"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="217dp"
            tools:layout_editor_absoluteY="70dp" />

        <RadioButton
            android:id="@+id/radiobutton10"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1" />

        <RadioButton
            android:id="@+id/radiobutton11"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1" />

    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="120dp"
        android:layout_height="51dp"
        android:layout_marginTop="25dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/radioGroup2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:orientation="horizontal">

        <RadioButton
            android:id="@+id/radiobutton1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:digits="1"
            tools:layout_editor_absoluteX="217dp"
            tools:layout_editor_absoluteY="70dp" />

        <RadioButton
            android:id="@+id/radiobutton2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1" />

        <RadioButton
            android:id="@+id/radiobutton3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1" />

    </RadioGroup>

    <RadioGroup
        android:id="@+id/radioGroup2"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/radioGroup3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup"
        tools:orientation="horizontal">

        <RadioButton
            android:id="@+id/radiobutton5"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="217dp"
            tools:layout_editor_absoluteY="70dp" />

        <RadioButton
            android:id="@+id/radiobutton6"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1" />

        <RadioButton
            android:id="@+id/radiobutton7"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_weight="1" />

    </RadioGroup>

    <Button
        android:id="@+id/contbut"
        android:layout_width="144dp"
        android:layout_height="47dp"
        android:layout_marginBottom="17dp"
        android:layout_marginEnd="16dp"
        android:text="Continue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="245dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:text="This is question 1, what is the answer?"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="245dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:text="This is question 2, what is the answer?"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="245dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:text="This is question 3, what is the answer?"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="245dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:text="This is question 4, what is the answer?"
        app:layout_constraintBottom_toTopOf="@+id/back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="45dp"
        android:text="1           2           3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="17dp"
        android:layout_marginStart="16dp"
        android:text="BACK"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="17dp"
        android:text="Submit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/contbut" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="1 = Not Yet, 2 = Sometimes, 3 = Often/Beyond:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>